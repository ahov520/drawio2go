{
  "name": "drawio2go",
  "version": "1.1.1",
  "description": "Electron + Next.js DrawIO Application",
  "main": "electron/main.js",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "eslint . && npx tsc --noEmit",
    "test": "vitest run",
    "format": "prettier --write --cache .",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:icons": "node scripts/generate-electron-icons.js",
    "electron:build": "npm run build && npm run electron:prepare && electron-builder",
    "electron:prepare": "node scripts/prepare-electron.js",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "electron-rebuild -f -w better-sqlite3"
  },
  "keywords": [
    "electron",
    "nextjs",
    "drawio",
    "heroui"
  ],
  "author": {
    "name": "menghuan",
    "email": "menghaun2003@outlook.com"
  },
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@ai-sdk/deepseek": "^2.0.1",
    "@ai-sdk/google": "^3.0.1",
    "@ai-sdk/openai": "^3.0.1",
    "@ai-sdk/openai-compatible": "^2.0.1",
    "@ai-sdk/react": "^3.0.3",
    "@heroui/react": "3.0.0-beta.2",
    "@heroui/styles": "3.0.0-beta.2",
    "@lobehub/icons": "^2.44.0",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@react-aria/interactions": "^3.25.6",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-virtual": "^3.13.12",
    "@xmldom/xmldom": "^0.8.11",
    "ai": "^6.0.3",
    "better-sqlite3": "^12.5.0",
    "date-fns": "^4.1.0",
    "diff-match-patch": "^1.0.5",
    "electron-store": "^11.0.2",
    "express": "^5.2.1",
    "i18next": "^25.7.1",
    "i18next-browser-languagedetector": "^8.2.0",
    "idb": "^8.0.3",
    "lucide-react": "^0.553.0",
    "next": "^15.5.7",
    "pako": "^2.1.0",
    "postcss": "^8.5.6",
    "react": "^19.2.1",
    "react-aria-components": "1.13.0",
    "react-dom": "^19.2.1",
    "react-drawio": "^1.0.7",
    "react-i18next": "^16.4.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.17",
    "uuid": "^11.1.0",
    "xpath": "^0.0.34",
    "yaml": "^2.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@fiahfy/icns": "^0.0.7",
    "@fiahfy/ico": "^0.0.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/diff-match-patch": "^1.0.36",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^10.0.0",
    "@types/xmldom": "^0.1.34",
    "concurrently": "^8.2.2",
    "electron": "^38.7.2",
    "electron-builder": "^24.13.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^15.5.7",
    "eslint-plugin-sonarjs": "^3.0.5",
    "jsdom": "^27.2.0",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.drawio2go.app",
    "productName": "DrawIO2Go",
    "asar": true,
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "server.js",
      ".next/**/*",
      "public/**/*"
    ],
    "files": [
      ".next/**/*",
      "!.next/cache/**/*",
      "public/**/*",
      "electron/**/*",
      "server.js",
      "next.config.mjs",
      "package.json",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "node_modules/better-sqlite3/build/Release",
        "to": ".",
        "filter": [
          "*.node"
        ]
      }
    ],
    "directories": {
      "buildResources": "electron/assets",
      "output": "dist"
    },
    "mac": {
      "icon": "electron/assets/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    },
    "win": {
      "icon": "electron/assets/icon.ico",
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "icon": "electron/assets/icon.png",
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    }
  }
}
